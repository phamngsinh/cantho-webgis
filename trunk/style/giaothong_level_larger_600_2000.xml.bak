<?xml version="1.0" encoding="UTF-8"?>
<StyledLayerDescriptor version="1.0.0" xsi:schemaLocation="http://www.opengis.net/sld StyledLayerDescriptor.xsd"
  xmlns="http://www.opengis.net/sld" xmlns:ogc="http://www.opengis.net/ogc" xmlns:xlink="http://www.w3.org/1999/xlink"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <NamedLayer>
    <Name>Simple Roads</Name>
    <UserStyle>
      <Title>Default Styler for simple road segments</Title>
      <Abstract>Light red line, 2px wide</Abstract>           
      
	<!--Begin Zoom Level 600000000-> 2000000000-->	
		<!--Mau duong vien ben ngoai o muc nay-->	
		<FeatureTypeStyle>              
        <Rule>          
          <Title>Mac dinh</Title>     
		<MinScaleDenominator>600000000</MinScaleDenominator>
		<MaxScaleDenominator>2000000000</MaxScaleDenominator>        	       
          <LineSymbolizer>
            <Stroke>
              <CssParameter name="stroke">#FFDB00</CssParameter>              
              <CssParameter name="stroke-width">10</CssParameter>
            </Stroke>
          </LineSymbolizer>                    
        </Rule>         
      </FeatureTypeStyle>
      
       <FeatureTypeStyle>              
        <Rule>
        <Title>Tinh lo</Title>
	 	<MinScaleDenominator>600000000</MinScaleDenominator>
		<MaxScaleDenominator>2000000000</MaxScaleDenominator>
          <ogc:Filter>
 	    <ogc:PropertyIsEqualTo>
              <ogc:PropertyName>loai_duong</ogc:PropertyName>
              <ogc:Literal>tinh lo</ogc:Literal>
 	    </ogc:PropertyIsEqualTo>
	  </ogc:Filter>
          
          <LineSymbolizer>
            <Stroke>
              <CssParameter name="stroke">#FFDB00</CssParameter>             	     
	      <CssParameter name="stroke-width">12</CssParameter>
            </Stroke>
          </LineSymbolizer>                   
        </Rule>         
      </FeatureTypeStyle>   
		<FeatureTypeStyle>              
        <Rule>
        <Title>Quoc lo</Title>
        <MinScaleDenominator>600000000</MinScaleDenominator>
		<MaxScaleDenominator>2000000000</MaxScaleDenominator>
		<ogc:Filter>
 	    <ogc:PropertyIsEqualTo>
              <ogc:PropertyName>loai_duong</ogc:PropertyName>
              <ogc:Literal>quoc lo</ogc:Literal>
 	    </ogc:PropertyIsEqualTo>
	  	</ogc:Filter>
          
          <LineSymbolizer>
            <Stroke>
              <CssParameter name="stroke">#FFDB00</CssParameter>             	     
	      <CssParameter name="stroke-width">13</CssParameter>
            </Stroke>
          </LineSymbolizer>                  
        </Rule>         
      </FeatureTypeStyle>
	<!--End Mau duong vien ben ngoai o muc nay-->	
	<FeatureTypeStyle>              
        <Rule>          
          <Title>Mac dinh</Title>     
		<MinScaleDenominator>600000000</MinScaleDenominator>
		<MaxScaleDenominator>2000000000</MaxScaleDenominator>        	       
          <LineSymbolizer>
            <Stroke>
              <CssParameter name="stroke">#BEBAED</CssParameter>              
              <CssParameter name="stroke-width">19</CssParameter>
            </Stroke>
          </LineSymbolizer>                    
        </Rule>         
      </FeatureTypeStyle>
      
       <FeatureTypeStyle>              
        <Rule>
        <Title>Tinh lo</Title>
	 	<MinScaleDenominator>600000000</MinScaleDenominator>
		<MaxScaleDenominator>2000000000</MaxScaleDenominator>
          <ogc:Filter>
 	    <ogc:PropertyIsEqualTo>
              <ogc:PropertyName>loai_duong</ogc:PropertyName>
              <ogc:Literal>tinh lo</ogc:Literal>
 	    </ogc:PropertyIsEqualTo>
	  </ogc:Filter>
          
          <LineSymbolizer>
            <Stroke>
              <CssParameter name="stroke">#C0E0E6</CssParameter>             	     
	      <CssParameter name="stroke-width">11</CssParameter>
            </Stroke>
          </LineSymbolizer>                   
        </Rule>         
      </FeatureTypeStyle>   
		<FeatureTypeStyle>              
        <Rule>
        <Title>Quoc lo</Title>
        <MinScaleDenominator>600000000</MinScaleDenominator>
		<MaxScaleDenominator>2000000000</MaxScaleDenominator>
		<ogc:Filter>
 	    <ogc:PropertyIsEqualTo>
              <ogc:PropertyName>loai_duong</ogc:PropertyName>
              <ogc:Literal>quoc lo</ogc:Literal>
 	    </ogc:PropertyIsEqualTo>
	  	</ogc:Filter>
          
          <LineSymbolizer>
            <Stroke>
              <CssParameter name="stroke">#C0E0E6</CssParameter>             	     
	      <CssParameter name="stroke-width">12</CssParameter>
            </Stroke>
          </LineSymbolizer>                  
        </Rule>         
      </FeatureTypeStyle>	
	<!--End Zoom Level 600000000-> 2000000000-->			
    </UserStyle>
  </NamedLayer>
</StyledLayerDescriptor>

<?xml version="1.0" encoding="UTF-8"?>
<StyledLayerDescriptor version="1.0.0" xsi:schemaLocation="http://www.opengis.net/sld StyledLayerDescriptor.xsd"
  xmlns="http://www.opengis.net/sld" xmlns:ogc="http://www.opengis.net/ogc" xmlns:xlink="http://www.w3.org/1999/xlink"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <NamedLayer>
    <Name>Simple Roads</Name>
    <UserStyle>
      <Title>Default Styler for simple road segments</Title>
      <Abstract>Duong di o muc zoom lon hon hoac bang 2000</Abstract>           

	<!--Begin Zoom Level Larger 2000000000-->	
	<!--Duong vien ben ngoai 2000000000-->	
	 <FeatureTypeStyle>              
        <Rule>          
        <Title>Mac Dinh</Title>     
		<MinScaleDenominator>2000000000</MinScaleDenominator>		
          <LineSymbolizer>
            <Stroke>
              <CssParameter name="stroke">#FFDB00</CssParameter>              
              <CssParameter name="stroke-width">3</CssParameter>
            </Stroke>
          </LineSymbolizer>    
        </Rule>  		       
      </FeatureTypeStyle>  

      <FeatureTypeStyle>              
       <Rule>
        <Title>Tinh lo</Title>
	 	<MinScaleDenominator>2000000000</MinScaleDenominator>		
        <ogc:Filter>
	 	    <ogc:PropertyIsEqualTo>
	              <ogc:PropertyName>loai_duong</ogc:PropertyName>
	              <ogc:Literal>tinh lo</ogc:Literal>
	 	    </ogc:PropertyIsEqualTo>
		 </ogc:Filter>          
          <LineSymbolizer>
            <Stroke>
              <CssParameter name="stroke">#FFDB00</CssParameter>             	     
	      	  <CssParameter name="stroke-width">4</CssParameter>
            </Stroke>
          </LineSymbolizer>          
        </Rule>         
      </FeatureTypeStyle>   
	  <FeatureTypeStyle>              
       <Rule>
        <Title>Quoc lo</Title>
        <MinScaleDenominator>2000000000</MinScaleDenominator>		
		<ogc:Filter>
 	    <ogc:PropertyIsEqualTo>
              <ogc:PropertyName>loai_duong</ogc:PropertyName>
              <ogc:Literal>quoc lo</ogc:Literal>
 	    </ogc:PropertyIsEqualTo>
	  	</ogc:Filter>          
          <LineSymbolizer>
            <Stroke>
              <CssParameter name="stroke">#FFDB00</CssParameter>             	     
	      	  <CssParameter name="stroke-width">5</CssParameter>
            </Stroke>
          </LineSymbolizer>          
        </Rule>         
      </FeatureTypeStyle>
	<!--End Duong vien ben ngoai 2000000000-->
	 <FeatureTypeStyle>              
        <Rule>          
        <Title>Mac Dinh</Title>     
		<MinScaleDenominator>2000000000</MinScaleDenominator>		
          <LineSymbolizer>
            <Stroke>
              <CssParameter name="stroke">#BEBAED</CssParameter>              
              <CssParameter name="stroke-width">2</CssParameter>
            </Stroke>
          </LineSymbolizer>    
        </Rule>  		       
      </FeatureTypeStyle>  

      <FeatureTypeStyle>              
       <Rule>
        <Title>Tinh lo</Title>
	 	<MinScaleDenominator>2000000000</MinScaleDenominator>		
        <ogc:Filter>
	 	    <ogc:PropertyIsEqualTo>
	              <ogc:PropertyName>loai_duong</ogc:PropertyName>
	              <ogc:Literal>tinh lo</ogc:Literal>
	 	    </ogc:PropertyIsEqualTo>
		 </ogc:Filter>          
          <LineSymbolizer>
            <Stroke>
              <CssParameter name="stroke">#C0E0E6</CssParameter>             	     
	      	  <CssParameter name="stroke-width">3</CssParameter>
            </Stroke>
          </LineSymbolizer>          
        </Rule>         
      </FeatureTypeStyle>   
	  <FeatureTypeStyle>              
       <Rule>
        <Title>Quoc lo</Title>
        <MinScaleDenominator>2000000000</MinScaleDenominator>		
		<ogc:Filter>
 	    <ogc:PropertyIsEqualTo>
              <ogc:PropertyName>loai_duong</ogc:PropertyName>
              <ogc:Literal>quoc lo</ogc:Literal>
 	    </ogc:PropertyIsEqualTo>
	  	</ogc:Filter>          
          <LineSymbolizer>
            <Stroke>
              <CssParameter name="stroke">#C0E0E6</CssParameter>             	     
	      	  <CssParameter name="stroke-width">4</CssParameter>
            </Stroke>
          </LineSymbolizer>          
        </Rule>         
      </FeatureTypeStyle>
	<!--End Zoom Level Larger 2000000000-->	
		
    </UserStyle>
  </NamedLayer>
</StyledLayerDescriptor>

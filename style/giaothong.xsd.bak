	<!--Begin : Chieu cua duong (arrow)-->
	<FeatureTypeStyle>
	<Rule>
	  <ogc:Filter>
	    <ogc:PropertyIsEqualTo>
	      <ogc:PropertyName>mot_chieu</ogc:PropertyName>
	      <ogc:Literal>1</ogc:Literal>
	    </ogc:PropertyIsEqualTo>
	  </ogc:Filter>
	  <TextSymbolizer>
		<Label>&#x2192;</Label>
			<Font>
				<CssParameter name="font-family">OpenSymbol</CssParameter>
				<CssParameter name="font-size">15</CssParameter>
				<CssParameter name="font-weight">normal</CssParameter>
			</Font>
		<LabelPlacement>
			<LinePlacement>
			</LinePlacement>
		</LabelPlacement>
		<Halo>
			<Radius>
				<ogc:Literal>mot_chieu</ogc:Literal>
			</Radius>
			<Fill>
				<CssParameter name="fill">#FFFFFF</CssParameter>
				<CssParameter name="fill-opacity">0.85</CssParameter>
			</Fill>
		</Halo>
		<Fill>
			<CssParameter name="fill">#225486</CssParameter>
		</Fill>
		<VendorOption name="forceLeftToRight">false</VendorOption>
		<!--Khoang cach toi da co the dich chuyen khi bi overlap-->
		<VendorOption name="maxDisplacement">100</VendorOption>
		<!--Hien thi ten lap lai moi 150 pixels-->
		<VendorOption name="repeat">150</VendorOption>
		<!--Khong cho overlap trong ban kinh 10 pixels-->
		<!--<VendorOption name="spaceAround">10</VendorOption>-->
		<!--Nhom cac nhan co cung ten lai voi nhau-->
		<VendorOption name="group">yes</VendorOption>
		<LabelPlacement>
  			<LinePlacement/>
		</LabelPlacement>
	  </TextSymbolizer>
	</Rule>
